#
# cron file(s) for individual of rsnapshot
#
# /etc/cron.d/rsnapshot-<user1>
# /etc/cron.d/rsnapshot-<user2>
#

30 */4	* * *		<%= name %>	/usr/bin/rsnapshot sync && /usr/bin/rsnapshot hourly
15  21  * * *		<%= name %>	/usr/bin/rsnapshot daily
15  22  * * 1		<%= name %>	/usr/bin/rsnapshot weekly
15  23  1 * *		<%= name %>	/usr/bin/rsnapshot monthly
