#
# cron file(s) for individual of rsnapshot
#
# /etc/cron.d/rsnapshot-<user1>
# /etc/cron.d/rsnapshot-<user2>
#

30 */4	* * *		<%= name %>	/usr/bin/rsnapshot -c /home/<%= name %>/.rsnapshot/rsnapshot.conf sync && /usr/bin/rsnapshot -c /home/<%= name %>/.rsnapshot/rsnapshot.conf hourly
15  21  * * *		<%= name %>	/usr/bin/rsnapshot -c /home/<%= name %>/.rsnapshot/rsnapshot.conf daily
15  22  * * 1		<%= name %>	/usr/bin/rsnapshot -c /home/<%= name %>/.rsnapshot/rsnapshot.conf weekly
15  23  1 * *		<%= name %>	/usr/bin/rsnapshot -c /home/<%= name %>/.rsnapshot/rsnapshot.conf monthly
